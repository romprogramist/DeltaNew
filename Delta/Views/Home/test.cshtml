@{
    ViewData["Title"] = "Home Page";
    // Layout = null;
}



@section Styles {
    <link rel="stylesheet" href="~/css/swiper-bundle.min.css">
    <environment include="Development">
        <link rel="stylesheet" href="~/css/home.css" asp-append-version="true">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="/css/home-bundle.css" asp-append-version="true">
    </environment>
}
@section Scripts {
    <script src="~/js/swiper-bundle.min.js"></script>
    <environment include="Development">
        <script src="~/js/home.js" asp-append-version="true"></script>
        <script src="~/js/add-hover-class.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/home-bundle.js" asp-append-version="true"></script>
        <script src="~/js/add-hover-class-bundle.js" asp-append-version="true"></script>
    </environment>
}

<section class="list-companies">
    <table>
        <table class="table">
            <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Logo</th>
            </tr>
            </thead>
            <tbody class="list"></tbody>
        </table>
    </table>
</section>