@using Delta.Models.PartialModels
@using Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Home Page";
}


@section Styles {
    <environment include="Development">
        <link rel="stylesheet" href="~/css/our-production.css" asp-append-version="true">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="/css/our-production-bundle.css" asp-append-version="true">
    </environment>
}

@section Scripts {
    <environment include="Development">
        <script src="~/js/home.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/home-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/hematology.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/hematology-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/open-list.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/open-list-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/open-doc.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/open-doc-bundle.js" asp-append-version="true"></script>
    </environment>
    
}

@{
    await Html.RenderPartialAsync("NotMainSectionPartial", new NotMainSectionModal
    {
        Title = "Биохимия"
    });
}


<section id="at-the-right-edge" class="at-the-right-edge">
    <div class="items">
        <div class="header-title">
            <h3>
                Автоматический биохимический анализатор BioSystems A-15 (Biosystems)
            </h3>
        </div>
        <div class="wrapper-d">
            <div class="item-images">
                <img class="box1 img-min1" src="/images/biochemistry/biochemistryA15-1-1.jpg" alt="">
                <img class="box1 img-big" src="/images/biochemistry/biochemistryA15-11-1.jpg" alt="">
                <img class="box2 img-min" src="/images/biochemistry/biochemistryA15-2-1.jpg" alt="">
                <img class="box3 img-min img-c" src="/images/biochemistry/biochemistryA15-3-1.jpg" alt="">
                <img class="box4 img-min" src="/images/biochemistry/biochemistryA15-4-1.jpg" alt="">
            </div>
            <div class="item-title">
                <h3>Описание</h3>
                <ul class="big-list">
                    <li>Система для клинической химии и турбидиметрии</li>
                    <li>Технология Random Access</li>
                    <li>Полный спектр системных реагентов для Биохимии и Турбидиметрии</li>
                    <li>Высокая производительность</li>
                    <li>Большое количество реагентов и проб на борту прибора</li>
                    <li>Многоразовый метакрилатный реакционный ротор</li>
                    <li>Возможность расширить панель исследований в лаборатории</li>
                    <li>Минимальное обслуживание</li>
                    <li>Низкое потребление воды</li>
                    <li>Отсутствие расходных материалов</li>
                    <li>Малый объем реагента на тест. Уменьшение расходов лаборатории</li>
                    <li>Программное обеспечение с многочисленными конфигурациями и возможностями программирования</li>
