@using Delta.Models.PartialModels
@using Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Home Page";
}


@section Styles {
    <environment include="Development">
        <link rel="stylesheet" href="~/css/our-production.css" asp-append-version="true">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="/css/our-production-bundle.css" asp-append-version="true">
    </environment>
}

@section Scripts {
    <environment include="Development">
        <script src="~/js/home.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/home-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/hematology.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/hematology-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/open-list.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/open-list-bundle.js" asp-append-version="true"></script>
    </environment>
    <environment include="Development">
        <script src="~/js/open-doc.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/open-doc-bundle.js" asp-append-version="true"></script>
    </environment>
    
}

@{
    await Html.RenderPartialAsync("NotMainSectionPartial", new NotMainSectionModal
    {
        Title = "Биохимия"
    });
}


<section id="at-the-right-edge" class="at-the-right-edge">
    <div class="items">
        <div class="header-title">
            <h3>
                Автоматический биохимический анализатор BioSystems A-25 (Biosystems)
            </h3>
        </div>
        <div class="wrapper-d">
            <div class="item-images">
                <img class="box1 img-min1" src="/images/biochemistry/biochemistryA25-1-1.jpg" alt="">
                <img class="box1 img-big" src="/images/biochemistry/biochemistryA25-11-1.jpg" alt="">
                <img class="box2 img-min" src="/images/biochemistry/biochemistryA25-2-1.jpg" alt="">
                <img class="box3 img-min img-c" src="/images/biochemistry/biochemistryA25-3-1.jpg" alt="">
                <img class="box4 img-min" src="/images/biochemistry/biochemistryA25-4-1.jpg" alt="">
            </div>
            <div class="item-title">
                <h3>Описание</h3>
                <ul class="big-list">
                    <li>Система для клинической химии и турбидиметрии</li>
                    <li>Технология Random Access</li>
                    <li>Высокая производительность</li>
                    <li>Охлаждаемый штатив с 30-ю позициями для реагентов. Независимая подача энергии</li>
                    <li>Автоматическое управление системой подачи растворов для обеспечения оптимального выполнения задач в любое время</li>
                    <li>Оптическая система последнего поколения для обеспечения максимальной стабильности и долговечности при любых внешних условиях, отсутствие опасности рассеивания света</li>
                    <li>Многоразовый ротор, изготовленный из химически инертного метакрилата, позволяющий работать в УФ и видимом диапазоне. Автоматическая проверка чистоты ротора</li>
                    <li>Оборудование настроено на прямую подачу реагента в образец, таким образом максимально обеспечивается уменьшение «мертвых» объемов дорогостоящих реагентов</li>
                    <li>Уменьшено потребление воды; автоматические датчики для оповещения о недостаточных для работы объемах растворов</li>
                    <li>Высокоточная система дозирования, относительное отклонение 2% при объеме образца 3 мкл. Минимальный объем считывания 200 мкл</li>
                    <li>Отображение поступающей информации рабочего листа в режиме реального времени</li>
                    <li>Автоматическое распределение реагентов в штативе или предварительное программирование штатива реагентов оператором</li>
                    <li>Простое и удобное программное обеспечение, включающее огромное количество функций</li>
                    <li>Автоматическое ежедневное обслуживание, включая самопроверку и автоматическое включение/выключение</li>
                    <li>Удобное управление интерфейсом с системой обратной связи</li>
                </ul>
            </div>
            <div class="item-table">
                <div class="column column-1">
                    <div class="hed-card">
                        <p>Технческие характеристики</p>
                    </div>
