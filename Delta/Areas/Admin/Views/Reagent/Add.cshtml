@{
    ViewBag.Title = "Reagent edit";
}

@section Scripts {    
    <environment include="Development">
        <script src="~/js/admin/reagent.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/admin/reagent-bundle.js" asp-append-version="true"></script>
    </environment>
}


<form>
    <div class="button-and-header">
        <h2>
            Добавить реаген
        </h2>
        <button class="add-btn">
            Добавить
        </button>
    </div>
    <p class="successfully"></p>
    <label>
        <input id="name" type="text" name="name" placeholder="Название"/>
    </label>
    <label for="">
        <input id="CompanyId" type="text" name="CompanyId" placeholder="Компания">
        <datalist id="fruitsList">
        </datalist>
    </label>
    <div class="validation"></div>
    <a class="link-transition" href="/Admin/Reagent/Index">Таблица со списком реагентов</a>
</form>


